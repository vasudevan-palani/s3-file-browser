{
  "name": "s3_browser_app",
  "version": "1.2.1",
  "main": "dist-electron/main/index.js",
  "description": "S3 browser app",
  "author": "vasudevan.palani@gmail.com",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "repository": "https://github.com/vasudevan-palani/s3-file-browser",
  "electron-updater": {
    "repo": "vasudevan-palani/s3-file-browser",
    "provider": "github",
    "autoDownload": true,
    "autoInstall": true
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && DEBUG=electron-builder electron-builder",
    "build-win": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@vitejs/plugin-vue": "^4.3.3",
    "electron": "^26.0.0",
    "electron-builder": "^24.6.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "@vue-icons/feather": "^1.0.19",
    "aws-sdk": "^2.1466.0",
    "electron-drag": "^2.0.0",
    "element-plus": "^2.3.14",
    "multi-ini": "^2.3.2",
    "update-electron-app": "^2.0.1",
    "vue-codemirror6": "^1.1.27",
    "vue-json-editor": "^1.4.3",
    "vue-router": "^4.2.5",
    "vue3-ace-editor": "^2.2.3",
    "vue3-ts-jsoneditor": "^2.9.0"
  }
}
